<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="0dp"
    android:layout_margin="0dp"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".LaunchView" >

    <ScrollView
        android:id="@+id/scrollView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:padding="2dp"
            android:orientation="vertical" >
			<!-- ========================================================================= -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#007500"
                android:padding="5dp"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/TextView03"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="System Info"
                    android:textAppearance="?android:attr/textAppearanceLarge" />

                <Button
                    android:id="@+id/ButtonVer"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Version Test" />
                <Button
                    android:id="@+id/ButtonSimCard"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Sim Card Test" />
                <Button
                    android:id="@+id/ButtonSDCard"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="SD Card Test" />
                <Button
                    android:id="@+id/ButtonListMethods"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="System Service Class Info" />

            </LinearLayout>
			<!-- ========================================================================= -->
			<LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#B15BFF"
                android:padding="5dp"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/TextView02"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Audio"
                    android:textAppearance="?android:attr/textAppearanceLarge" />
                
                <Button
                    android:id="@+id/ButtonSpeaker"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Speaker Test" />

                <Button
                    android:id="@+id/ButtonReciver"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="2dp"
                    android:text="Reciver Test" />

                <Button
                    android:id="@+id/ButtonMMicRec"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Main MIC - Reciver Loopback" />

                <Button
                    android:id="@+id/ButtonEMic1Rec"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Extra MIC 1 - Reciver Loopback" />

                <Button
                    android:id="@+id/ButtonEMic2Rec"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Extra MIC 2 - Reciver Loopback" />
                <Button
                    android:id="@+id/ButtonHeadsetLoop"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Headset Loopback" />
                
                 

            </LinearLayout>
			<!-- ========================================================================= -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#007500"
                android:padding="5dp"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/TextView01"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Sensor"
                    android:textAppearance="?android:attr/textAppearanceLarge" />


                <Button
                    android:id="@+id/ButtonLSensor"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Light Sensor Test" />

                <Button
                    android:id="@+id/ButtonPSensor"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Proximity Sensor" />

                <Button
                    android:id="@+id/ButtonAcc"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:text="Accelerater Test" />

                <Button
                    android:id="@+id/ButtonGyro"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Gyroscope Test" />

                <Button
                    android:id="@+id/ButtonMag"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Magnetism Test" />

               
            </LinearLayout>
            
           <!-- ========================================================================= -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#B15BFF"
                android:padding="5dp"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/TextView01"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Radio"
                    android:textAppearance="?android:attr/textAppearanceLarge" />

                <Button
                    android:id="@+id/ButtonWifi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Wifi Test" />

                <Button
                    android:id="@+id/ButtonBlueTooth"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Bluetooth Test" />

                <Button
                    android:id="@+id/ButtonGPS"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:text="GPS Test" />

                <Button
                    android:id="@+id/ButtonFM"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="FM Test" />

             
            </LinearLayout>
            <!-- ========================================================================= -->
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#007500"
                android:padding="5dp"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/TextView01"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Miscellaneous"
                    android:textAppearance="?android:attr/textAppearanceLarge" />

                <Button
                    android:id="@+id/ButtonKeys"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Keys Test" />

                <Button
                    android:id="@+id/ButtonTP"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Touch Panel Test" />

                <Button
                    android:id="@+id/ButtonVib"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Vibrator Test" />
                
                <Button
                    android:id="@+id/ButtonPromptingLed"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Prompting Led Test" />
                
                <Button
                    android:id="@+id/ButtonFlashLed"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Flash Led Test" />

                <Button
                    android:id="@+id/ButtonBat"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Battery And Charger Test" />
				

                <Button
                    android:id="@+id/ButtonForeCamera"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Fore Camera Test" />
                
                <Button
                    android:id="@+id/ButtonBackCamera"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Back Camera Test" />
                
                <Button
                    android:id="@+id/ButtonLcdRed"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Lcd Red Test" />
                
                <Button
                    android:id="@+id/ButtonLcdBlue"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Lcd Blue Test" />
                
                <Button
                    android:id="@+id/ButtonLcdGreen"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:enabled="false"
                    android:layout_marginTop="2dp"
                    android:layout_marginBottom="2dp"
                    android:text="Lcd Green Test" />
             
            </LinearLayout>

        </LinearLayout>

    </ScrollView>

</RelativeLayout>
